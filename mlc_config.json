{
    "replacementPatterns": [
        {
          "pattern": "^/",
          "replacement": "https://docs.spectrocloud.com/"
        }
      ],
    "httpHeaders": [
      {
        "urls": ["https://github.com/", "https://guides.github.com/", "https://help.github.com/", "https://docs.github.com/"],
        "headers": {
          "Accept-Encoding": "zstd, br, gzip, deflate"
        }
      }
    ],
    "retryOn429": true,
    "retryCount": 3,
    "fallbackRetryDelay": "10s"
  }